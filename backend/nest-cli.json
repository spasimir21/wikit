{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "image": {
      "type": "application",
      "root": "apps/image",
      "entryFile": "main",
      "sourceRoot": "apps/image/src",
      "compilerOptions": {
        "tsConfigPath": "apps/image/tsconfig.app.json"
      }
    },
    "data": {
      "type": "application",
      "root": "apps/data",
      "entryFile": "main",
      "sourceRoot": "apps/data/src",
      "compilerOptions": {
        "tsConfigPath": "apps/data/tsconfig.app.json"
      }
    },
    "neo4ogm": {
      "type": "library",
      "root": "libs/neo4ogm",
      "entryFile": "index",
      "sourceRoot": "libs/neo4ogm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/neo4ogm/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "rating": {
      "type": "application",
      "root": "apps/rating",
      "entryFile": "main",
      "sourceRoot": "apps/rating/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rating/tsconfig.app.json"
      }
    },
    "search": {
      "type": "application",
      "root": "apps/search",
      "entryFile": "main",
      "sourceRoot": "apps/search/src",
      "compilerOptions": {
        "tsConfigPath": "apps/search/tsconfig.app.json"
      }
    },
    "elasticsearch": {
      "type": "library",
      "root": "libs/elasticsearch",
      "entryFile": "index",
      "sourceRoot": "libs/elasticsearch/src",
      "compilerOptions": {
        "tsConfigPath": "libs/elasticsearch/tsconfig.lib.json"
      }
    }
  }
}